{
  "name": "cap-snowflake",
  "version": "1.0.0",
  "description": "SAP CAP database adapter for Snowflake with full OData support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "NODE_OPTIONS='--loader tsx' mocha 'test/**/*.test.ts'",
    "test:unit": "NODE_OPTIONS='--loader tsx' mocha 'test/unit/**/*.test.ts'",
    "test:integ": "NODE_OPTIONS='--loader tsx' mocha 'test/integ/**/*.test.ts'",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "import-schema": "node dist/cli/import-schema.js"
  },
  "bin": {
    "cap-snowflake-import": "./dist/cli/import-schema.js"
  },
  "keywords": [
    "sap",
    "cap",
    "cds",
    "snowflake",
    "database",
    "adapter",
    "odata"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@sap/cds": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "snowflake-sdk": "^1.11.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.10",
    "eslint": "^8.0.0",
    "mocha": "^10.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@sap/cds": ">=7.0.0"
  },
  "cds": {
    "kind": "snowflake"
  }
}

